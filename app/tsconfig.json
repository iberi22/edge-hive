{
   "extends": "astro/tsconfigs/strict",
   "compilerOptions": {
      "baseUrl": ".",
      "paths": {
         "$lib": [
            "src/lib"
         ],
         "$lib/*": [
            "src/lib/*"
         ],
         "$components": [
            "src/components"
         ],
         "$components/*": [
            "src/components/*"
         ],
         "$layouts": [
            "src/layouts"
         ],
         "$layouts/*": [
            "src/layouts/*"
         ],
         "$pages": [
            "src/pages"
         ],
         "$pages/*": [
            "src/pages/*"
         ],
         "$stores": [
            "src/stores"
         ],
         "$stores/*": [
            "src/stores/*"
         ]
      },
      "jsx": "preserve",
      "jsxImportSource": "astro"
   }
}